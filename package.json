{
  "name": "@panhezeng/vue-axios",
  "description": "A Vue plugin",
  "version": "2.0.2",
  "author": "潘何增 <panhezeng@gmail.com> (http://apsay.com/)",
  "license": "MIT",
  "private": false,
  "keywords": [
    "vuejs",
    "vue plugin",
    "ajax",
    "axios"
  ],
  "homepage": "https://github.com/panhezeng/vue-axios#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panhezeng/vue-axios.git"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README"
  ],
  "main": "dist/vue-axios.min.js",
  "scripts": {
    "dev:example": "webpack-dev-server --mode development --open --hot --config webpack.example.config.js",
    "build:example": "rm -rf docs && mkdir docs && cp index.html docs && webpack --mode production --config webpack.example.config.js",
    "build": "webpack --mode production"
  },
  "peerDependencies": {
    "axios": "~0",
    "vue": "~2"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "~7",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/runtime": "^7.4.4",
    "@panhezeng/before-run-webpack-plugin": "~1",
    "axios": "~0",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.7",
    "prettier": "^1.17.1",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,json,css,md,jsx,vue,ts,less}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/**",
      "**/docs/**"
    ]
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 6.0.0"
  }
}
